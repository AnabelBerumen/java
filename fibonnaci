	//inicio metodo fibonacci
	public String fibonacci( int n) {
		
		String fibonacci = "";
		int x = 0;
		int y = 0;
		int z = 1;
		int i = 0;
		
		while ( i < n ){//inicio while
			
        x = x + y;
        y = z;
        z = x + y;
        fibonacci = fibonacci +" " + z ;
       
       i++;
		}//fin while
		
		return fibonacci;
	
	}//fin metodo fibonacci
